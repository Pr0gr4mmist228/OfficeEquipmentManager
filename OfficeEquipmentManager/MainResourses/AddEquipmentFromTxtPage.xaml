<?xml version="1.0" encoding="utf-8"?>
<Page
	x:Class="OfficeEquipmentManager.MainResourses.AddEquipmentFromTxtPage" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="AddEquipmentFromTxtPage">
	<Grid>
		<StackPanel
			Orientation="Horizontal"
			Margin="0,0,0,0"
			VerticalAlignment="Center"
			HorizontalAlignment="Center"
			Grid.Row="0"
			Grid.Column="0">
			<Button
				Name="buttonSetFilePath"
				Margin="0 0 10 0"
				Height="30"
				Click="ButtonSetFilePath_Click"
				Width="155">Указать путь к файлу</Button>
			<TextBox
				Name="textBoxFilePath"
				Width="110" />
		</StackPanel>
	</Grid>
</Page>