<?xml version="1.0" encoding="utf-8"?>
<Window
	x:Class="OfficeEquipmentManager.AdministratorResourses.AddCategoryWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="OfficeEquipmentManager.AdministratorResourses"
	Width="500"
	Height="400">
	<Grid>
		<StackPanel
			VerticalAlignment="Top">
			<StackPanel>
				<TextBlock
					TextAlignment="Center">Введите имя категории</TextBlock>
				<TextBox
					Margin="0 5 0 0"
					Name="textBoxCatrogyName" />
			</StackPanel>
			<Button
				Height="40"
				Width="180"
				Margin="0 5 0 0"
				Name="buttonAddCategory"
				Click="ButtonAddCategory_Click">Добавить категорию</Button>
		</StackPanel>
	</Grid>
</Window>